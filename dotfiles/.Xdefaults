! This is a comment ;-)
!
#ifdef COLOR
   *customization: -color
#endif
!
*visualBell: True
!
XTerm*saveLines:    1500
XTerm*titlebar:         on
XTerm.ActiveIcon:       on
XTerm.AllowIconInput:   on
XTerm.Border:           blue
XTerm.BorderWidth:      5
XTerm*internalBorder:   2
XTerm*pointerColor: red
xterm*iconPixmap: /home/zymos/Documents/share/xterm_32x32.xbm
xterm*iconMask: /home/zymos/Documents/share/xterm_32x32-mask.xbm

!XTerm*pointerColorBackground: white
XTerm*cursorColor: #00cc00
XTerm*loginShell: true
XTerm*scrollBar: false
XTerm*scrollKey: true
XTerm*multiClickTime: 250
! XTerm*pointerShape: bogosity

!This color scheme makes a really nice dark one, works well with vim->darkelf
XTerm*background:  #000000
XTerm*foreground:  #9f9f9f
XTerm*color0:      #000000
XTerm*color1:      #9e1828
XTerm*color2:      #aece92
XTerm*color3:      #968a38
XTerm*color4:      #414171
XTerm*color5:      #963c59
XTerm*color6:      #418179
XTerm*color7:      #bebebe
XTerm*color8:      #666666
XTerm*color9:      #cf6171
XTerm*color10:     #c5f779
XTerm*color11:     #fff796
XTerm*color12:     #4186be
XTerm*color13:     #cf9ebe
XTerm*color14:     #71bebe
XTerm*color15:     #ffffff

XTerm*allowSendEvents: true

! I use rxvt with rtorrent to get a different term color and icon, than my xterm
rxvt*geometry: 105x42
rxvt*background:   #225022
rxvt*foreground:   #ffffff
rxvt*cursorColor:  #ffffff
rxvt*pointerColor: #00ff00
rxvt*font: 6x13 
